{
  "name": "roumuter",
  "version": "0.0.3",
  "description": "Support push state to history without navigate.",
  "scripts": {
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:es": "babel src --out-dir es",
    "build:umd": "cross-env NODE_ENV=development rollup -c -o dist/react-light-state.js",
    "build:umd:min": "cross-env NODE_ENV=production rollup -c -o dist/react-light-state.min.js",
    "build": "npm run build:commonjs && npm run build:es && npm run build:umd && npm run build:umd:min",
    "clean": "rimraf lib dist es coverage",
    "format": "prettier --write \"{src,test}/**/*.{js,ts}\" index.d.ts \"docs/**/*.md\"",
    "lint": "eslint src test/utils test/components",
    "prepare": "npm run clean && npm run build",
    "pretest": "npm run lint",
    "test": "jest",
    "coverage": "codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fozg/roumuter.git"
  },
  "author": "Fozg <phongdaohonng@gmail.com> (https://github.com/fozg)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fozg/roumuter/issues"
  },
  "main": "./lib/index.js",
  "unpkg": "dist/react-light-state.js",
  "module": "es/index.js",
  "files": [
    "dist",
    "lib",
    "src",
    "es"
  ],
  "homepage": "https://github.com/fozg/roumuter#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/plugin-transform-react-display-name": "^7.2.0",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "jest": "^24.8.0",
    "jest-environment-jsdom": "^24.8.0",
    "jest-environment-jsdom-global": "^1.2.0",
    "jsdom": "^15.1.0",
    "cross-env": "^5.2.0",
    "rollup": "^1.8.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.3.3",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-replace": "^2.1.1",
    "rollup-plugin-terser": "^4.0.4"
  },
  "dependencies": {
    "query-string": "^6.5.0"
  },
  "jest": {
    "testEnvironment": "jest-environment-jsdom-global"
  },
  "windows": {
    "program": "${workspaceFolder}/node_modules/jest-cli/bin/jest"
  },
  "browserify": {
    "transform": [
      "loose-envify"
    ]
  }
}
